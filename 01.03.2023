int main()
{
    int x;
    scanf("%d", &x);
    if(x>=0)
    {
        printf("|%d|=%d",x,x);
    }
    else
    {
        printf("|%d|=%d",x,-x);
    }
    return 0;
}

int main()
{
    int x, y, z;
    scanf("%d%d%d", &x,&y,&z);
    if(x>y && x>z)
    {
        printf("%d",x);
    }
    else if(x<y && y > z)
    {
        printf("%d",y);
    }
    else if(x<z && z>y)
    {
        printf("%d",z);
    }
    return 0;
}
